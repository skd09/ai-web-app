datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}
generator client {
  provider = "prisma-client-js"
}

model UserApiLimit{
  id Int @default(autoincrement()) @id
  userId String @unique
  count Int @default(0)
  createdAt DateTime @default(now())
  updateAt DateTime @updatedAt 
}
